{
  "compilerOptions": {
    "target": "es6",
    "lib": [
      "es2020"
    ],
    "module": "commonjs",
    "esModuleInterop": true,
    "moduleResolution": "node",
    "strict": true,
    "sourceMap": false,
    "declaration": true,
    "declarationMap": false,
    "noImplicitAny": true,
    "noUnusedLocals": true,
    "noImplicitReturns": true,
    "noUnusedParameters": true,
    "forceConsistentCasingInFileNames": true
  }
}
